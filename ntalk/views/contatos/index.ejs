<%- include('../home/header'); %>
        <header>
                <h1>Ntalk</h1>
                <h4>Agenda de contatos</h4>
        </header>
        <section>
                <form action="/contato" method="post">
                        <input type="text" name="contato[nome]" placeholder="Nome">
                        <input type="" text name="contato[email]" placeholder="Email">
                        <button type="submit">Cadastrar</button>
                </form>
                <p>Bem-vindo <%- usuario.nome %>
                </p>
                <p>Contatos do usuario: <% contatos.forEach(function(contato,index){ %>
                        <tr>
                                <th><%- contato.nome %></th>
                        </tr>
                        <% }) %>
                </p>
        </section>
        <%- include('../home/exit'); %>
                <%- include('../home/footer'); %>